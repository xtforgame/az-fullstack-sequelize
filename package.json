{
  "name": "az-fullstack-sequelize",
  "version": "1.0.0",
  "description": "A fullstack sequelize seed project",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "gulp",
    "seed",
    "boilerplate",
    "kickstarter",
    "ES6",
    "6to5"
  ],
  "author": "Rick Chen <xtforgame@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js ./src && tslint -c ./tslint.json './src/**/*.ts'",
    "lint-fix": "eslint --fix --ext .js ./src && tslint --fix -c ./tslint.json './src/**/*.ts'",
    "lint-all": "eslint --ext .js ./src ./test ./internals && tslint -c ./tslint.json './src/**/*.ts'",
    "lint-all-fix": "eslint --fix --ext .js ./src ./test ./internals&& tslint --fix -c ./tslint.json './src/**/*.ts'",
    "build": "cross-env NODE_ENV=production gulp server:build && cross-env NODE_ENV=production gulp server:build:extras && cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "build-server": "cross-env NODE_ENV=production gulp server:build && cross-env NODE_ENV=production gulp server:build:extras",
    "build-server-dev": "cross-env NODE_ENV=development gulp server:build && cross-env NODE_ENV=development gulp server:build:extras",
    "start": "cross-env NODE_ENV=development gulp server:watch:dev",
    "debug": "cross-env NODE_ENV=development gulp server:watch:dev --inspect",
    "test:server": "eslint --ext .js ./test/server && cross-env NODE_ENV=test mocha --require ./test/babel-register.js './test/server/**/*.spec.js'",
    "test-watch:server": "eslint --ext .js ./test/server && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js,ts --require ./test/babel-register.js './test/server/**/*.spec.js'",
    "test:front-end": "eslint --ext .js ./test/front-end && cross-env NODE_ENV=test mocha --require ignore-styles --require ./test/babel-register.js './test/front-end/**/*.spec.js'",
    "test-watch:front-end": "eslint --ext .js ./test/front-end && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js,ts --require ignore-styles --require ./test/babel-register.js './test/front-end/**/*.spec.js'",
    "test:hasura": "eslint --ext .js ./test/hasura && cross-env NODE_ENV=test mocha --require ./test/babel-register.js './test/hasura/**/*.spec.js'",
    "test-watch:hasura": "eslint --ext .js ./test/hasura && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js,ts --require ./test/babel-register.js './test/hasura/**/*.spec.js'",
    "gmail-auth": "cross-env NODE_ENV=production node dist/server/scripts/gmailAuth.js",
    "gmail-auth-dev": "cross-env NODE_ENV=development node dist/server/scripts/gmailAuth.js",
    "start-db": "docker-compose -f dev.yml up -d",
    "stop-db": "docker-compose -f dev.yml down",
    "config": "cross-env HYGEN_OVERWRITE=1 hygen deploy config",
    "c-cmd": "cross-env NODE_ENV=production node dist/server/scripts/c-cmd",
    "c-cmd-dev": "cross-env NODE_ENV=development node dist/server/scripts/c-cmd",
    "amm-sync": "cross-env NODE_ENV=development node dist/server/scripts/ammSync/index.js",
    "start-mig": "docker-compose -f migration.yml up -d",
    "stop-mig": "docker-compose -f migration.yml down",
    "init:hasura": "yarn stop-db&&yarn start-db&&yarn build-server-dev&&yarn test:hasura&&yarn amm-sync&&yarn graphql-codegen",
    "test:migration": "cross-env NODE_ENV=test mocha --require ./test/babel-register.js './test/migration/**/*.spec.ts'"
  },
  "license": "MIT",
